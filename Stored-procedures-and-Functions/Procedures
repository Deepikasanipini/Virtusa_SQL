create or replace procedure empproc(a in number,b out varchar2)
is 
begin
    select ename into b from emp where eno=a;
end;
/

create or replace procedure count1 as
begin
dbms_output.put_line('Welcome to procedures');
end;

set serveroutput on;
execute count1;
